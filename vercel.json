{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "src/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "prod",
    "PORT": "5000",
    "API_BASE_URL": "@API_BASE_URL",
    "MONGO_URI": "@MONGODB_URI",
    "JWT_SECRET": "@JWT_SECRET"
  }
}
